<akira>
  <rule src="lofusz">
    <sum>
      <any ref="a" post="return a.Name == &quot;num&quot;;"/>
      <any ref="b"/> 
    </sum>
    <mul>
      <num>2</num>
      <any ref="a"/>
      <any ref="b"/>
    </mul>
  </rule>
  <apply src="lofusz"/>
  <sum>
    <num>7</num>
    <num>8</num>
  </sum>
</akira>